{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "airbnbBookingDataAnalytics"
		},
		"AirbnbCosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AirbnbCosmosDB'"
		},
		"CustomerDataADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'CustomerDataADLS'"
		},
		"dwh_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dwh'"
		},
		"CustomerDataADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://strgacct0101.dfs.core.windows.net/"
		},
		"dwh_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "poonamtest.database.windows.net"
		},
		"dwh_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "poonamtestdb"
		},
		"dwh_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "airbnbBookingDataAnalytics_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AirbnbCosmosDB_connectionString": {
						"value": "[parameters('AirbnbCosmosDB_connectionString')]"
					},
					"CustomerDataADLS_accountKey": {
						"value": "[parameters('CustomerDataADLS_accountKey')]"
					},
					"dwh_password": {
						"value": "[parameters('dwh_password')]"
					},
					"CustomerDataADLS_properties_typeProperties_url": {
						"value": "[parameters('CustomerDataADLS_properties_typeProperties_url')]"
					},
					"dwh_properties_typeProperties_server": {
						"value": "[parameters('dwh_properties_typeProperties_server')]"
					},
					"dwh_properties_typeProperties_database": {
						"value": "[parameters('dwh_properties_typeProperties_database')]"
					},
					"dwh_properties_typeProperties_userName": {
						"value": "[parameters('dwh_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}